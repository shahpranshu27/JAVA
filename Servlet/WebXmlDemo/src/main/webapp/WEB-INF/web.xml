<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="https://jakarta.ee/xml/ns/jakartaee"
	xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd"
	id="WebApp_ID" version="6.0">

	<servlet-mapping>
		<servlet-name>Login1</servlet-name>
		<url-pattern>/myLogin</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Login1</servlet-name>
		<servlet-class>in.sp.backend.Login1</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Register1</servlet-name>
		<url-pattern>/myReg</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Register1</servlet-name>
		<servlet-class>in.sp.backend.Register1</servlet-class>
	</servlet>
	
</web-app>